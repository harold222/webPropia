
{{> header}}

    <main class="site-main">
<div class="ctn_projects">
    <div class="file">
        <div class="projects_desc">
            <h1>{{userWeb.nombreProject}}</h1>
            <p>
                {{userWeb.descProject}}
                Fue Creado el: {{userWeb.dateDevelopment}}
            </p>
            <a href="{{userWeb.link}}">Ver El Proyecto...</a>
        </div>

        <div class="projects_languages">
            <div class="projects_img">
                    <img src="{{userWeb.imgComplete}}" alt="My project">
                <div class="zoom_icon">
                    <a class="venobox" data-gall="images" href="{{userWeb.imgComplete}}">
                        <i class="fas fa-search-plus fa-4x"></i>
                    </a>
                </div>
            </div>
            <div class="languages">
                <div class="scroll">
                    <ul>
                        {{#each userWeb}}
                            <li>
                                <a href="#">
                                    <i class="{{this.technologies}} fa-2x"></i> {{this.icons}}
                                </a>
                            </li>
                        {{/each}}
                    </ul>
                </div>
            </div>
        </div>
        
    </div>
</div>
{{> footer}}